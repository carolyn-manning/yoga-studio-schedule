<% if @teacher.errors.any? %>
  <% @teacher.errors.full_messages.each do |message|%>
  <li><%= message %> </li>
  <%end%>
<%end%>

<%= form_for @teacher  do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <br/>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <br/>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
    <br/>
  
    <%= f.submit %>
<% end %>