<h1> <%= @teacher.first_name %> <%= @teacher.last_name %> </h1> 

<h2> <%= @teacher.first_name %>'s Styles: </h2> 
<% @teacher.styles.uniq.each do |style| %>
    <li> 
    <%= style.name %> 
    </li>
<% end %>

<h2> <%= @teacher.first_name %>'s Upcoming Classes </h2> 
<% @teacher.yoga_classes.each do |yoga_class| %>
    <li> 
    <%= yoga_class.date %> 
    <%= yoga_class.time %> 
    <%= yoga_class.style.name %> 
    <%= yoga_class.teacher.first_name %> 
    </li>
<% end %>
