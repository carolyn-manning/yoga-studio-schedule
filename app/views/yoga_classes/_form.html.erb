<%= form_for @yoga_class  do |f| %>
    <%= f.label :date %>
    <%= f.date_field :date %>
    <br/>
    <%= f.label :time %>
    <%= f.time_field :time %>
    <br/>
    <%= f.label :style_id %>
    <%= f.collection_select :style_id, Style.all, :id, :name %>
    <br/>
    <%= f.label :teacher_id %>
    <%= f.collection_select :teacher_id, Teacher.all, :id, :first_name %>
    <br/>
  
    <%= f.submit %>
<% end %>